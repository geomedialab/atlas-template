{
	"canvasType": "map",
	"canvasName": "mapStories",
	"coordinates": {
		"initialBounds": [
			-157,
			-32,
			75,
			67
		],
		"srsName": "EPSG:4326",
		"autoInitialBounds": false
	},
	"initialFeatureDisplay": "all",
	"initialCinemapToMapSettings": {
		"fitMap": true,
		"animateFitMap": true,
		"displayMedia": true,
		"zoomThreshold": false
	},
	"renderOrderBasedOn": "=doc._ldata.start",
	"backgrounds": [
		{
			"name": "Stamen Terrain",
			"type": "stadia",
			"options": {
				"layerName": "stamen_terrain"
			}
		},
		{
			"name": "Stamen Toner",
			"type": "stadia",
			"options": {
				"layerName": "stamen_toner"
			}
		},
		{
			"name": "Stamen Watercolour",
			"type": "stadia",
			"options": {
				"layerName": "stamen_watercolor"
			}
		},
		{
			"name": "OSM Bright",
			"type": "stadia",
			"options": {
				"layerName": "osm_bright"
			}
		},
		{
			"name": "OSM",
			"type": "osm",
			"options": {
				"url": "https://{a-c}.tile.openstreetmap.org/{z}/{x}/{y}.png"
			}
		},
		{
			"name": "Bing Satellite",
			"type": "bing",
			"options": {
				"key": "your key goes here",
				"imagerySet": "Aerial"
			}
		}
	],
	"overlays": [
		{
			"id": "firstOverlay",
			"name": {
				"nunaliit_type": "localized",
				"en": "Demo"
			},
			"type": "model",
			"options": {
				"sourceModelId": "dualFilteredDonuts"
			},
			"featurePopupDelayMs": 300,
			"useHoverSound": true,
			"clustering": {
				"distance": 20,
				"threshold": 1,
				"disableDynamicClustering": false,
				"clusterPointsOnly": false,
				"minimumLinePixelSize": 20,
				"minimumPolygonPixelSize": 20
			}
		}
	],
	"addPointsOnly": false,
	"showSRSAttribution": true,
	"toggleClick": true,
	"styles": [
		{
			"condition": "hasProperty('doc._ldata')",
			"normal": {
				"v2_style": true,
				"n2es6.n2mapModule.N2CustomPointStyle": {
					"type": "treeRingB",
					"donutScaleFactor": "=doc._ldata.scaleFactor",
					"color": "=doc._ldata.style.fillColor",
					"stroke": "#ffffff",
					"opacity": 0.8,
					"data": "=doc._ldata",
					"text": "",
					"startupOffset": 5
				}
			},
			"selected": {
				"v2_style": true,
				"n2es6.n2mapModule.N2CustomPointStyle": {
					"type": "treeRingB",
					"donutScaleFactor": "=doc._ldata.scaleFactor",
					"color": "=doc._ldata.style.fillColor",
					"stroke": "#ffffff",
					"opacity": 0.8,
					"data": "=doc._ldata",
					"text": "",
					"startupOffset": 5
				}
			},
			"hovered": {
				"v2_style": true,
				"n2es6.n2mapModule.N2CustomPointStyle": {
					"type": "treeRingB",
					"donutScaleFactor": "=doc._ldata.scaleFactor",
					"color": "=doc._ldata.style.fillColor",
					"stroke": "#000000",
					"data": "=doc._ldata",
					"startupOffset": 5
				}
			}
		}
	]
}
