{
    "canvasType": "map",
    "canvasName": "multiMapStories",
    "coordinates": {
        "initialBounds": [
            -157,
            -32,
            75,
            67
        ],
        "srsName": "EPSG:4326",
        "autoInitialBounds": false
    },
    "initialFeatureDisplay": "donut",
    "initialCinemapToMapSettings": {
	"fitMap": true,
	"animateFitMap": true,
	"displayMedia": true,
	"zoomThreshold": false
    },
    "renderOrderBasedOn": "=doc._ldata.start",
    "backgrounds": [
		{
			"name": "Stamen TonerLite",
			"type": "stamen",
			"options": {
				"url": "https://stamen-tiles-{s}.a.ssl.fastly.net/toner-lite/{z}/{x}/{y}{r}.{ext}",
				"layerName": "toner-lite"
			}
		},
		{
			"name": "Stamen Toner",
			"type": "stamen",
			"options": {
				"url": "https://stamen-tiles-{s}.a.ssl.fastly.net/toner/{z}/{x}/{y}{r}.png",
				"layerName": "toner"
			}
		},
		{
			"name": "Stamen Terrain",
			"type": "stamen",
			"options": {
				"url": "https://stamen-tiles-{s}.a.ssl.fastly.net/terrain/{z}/{x}/{y}{r}.{ext}",
				"layerName": "terrain"
			}
		},
		{
			"name": "Stamen Watercolor",
			"type": "stamen",
			"options": {
				"url": "https://stamen-tiles-{s}.a.ssl.fastly.net/watercolor/{z}/{x}/{y}.{ext}",
				"layerName": "watercolor"
			}
		},
		{
			"name": "OSM",
			"type": "osm",
			"options": {
				"url": "https://{a-c}.tile.openstreetmap.org/{z}/{x}/{y}.png"
			}
		},
		{
			"name": "Bing Satellite",
			"type": "bing",
			"options": {
				"key": "AvyVLcoQFJ26o6n637tAhWevQeu4GZiwtq7ldGccby46Fao6yq1MyDszdp_8MMsA",
				"imagerySet": "Aerial"
			}
		}

    ],
    "overlays": [
        {
            "id": "firstOverlay",
            "name": {
                "nunaliit_type": "localized",
                "en": "Demo"
            },
            "type": "model",
            "options": {
                "sourceModelId": "themeDonutFilterByGroupTag"
            },
            "featurePopupDelayMs": 300,
            "useHoverSound": true,
            "clustering": {
                "distance": 20,
                "threshold": 1,
                "disableDynamicClustering": false,
                "clusterPointsOnly": false,
                "minimumLinePixelSize": 20,
                "minimumPolygonPixelSize": 20
            }
        }
    ],
    "addPointsOnly": false,
    "showSRSAttribution": true,
    "toggleClick": true,
    "styles": [
        {
            "condition": "hasProperty('doc._ldata')",
            "normal": {
                "v2_style": true,
                "n2es6.n2mapModule.N2CustomPointStyle": {
                    "type": "treeRingB",
                    "donutScaleFactor": "=doc._ldata.scaleFactor",
                    "color": "=doc._ldata.style.fillColor",
                    "stroke": "#ffffff",
                    "opacity": 0.8,
                    "data": "=doc._ldata",
                    "text": "",
                    "startupOffset": 5
                }
            },
            "selected": {
                "v2_style": true,
                "n2es6.n2mapModule.N2CustomPointStyle": {
                    "type": "treeRingB",
                    "donutScaleFactor": 10,
                    "data": "=doc._ldata",
                    "color": "#ffff33",
                    "opacity": 0.5,
                    "startupOffset": 5
                }
            },
            "hovered": {
                "v2_style": true,
                "n2es6.n2mapModule.N2CustomPointStyle": {
                    "type": "treeRingB",
                    "donutScaleFactor": "=doc._ldata.scaleFactor",
                    "stroke": "#000000",
                    "color": "=doc._ldata.style.fillColor",
                    "data": "=doc._ldata",
                    "startupOffset": 5
                }
            }
        }
    ]
}