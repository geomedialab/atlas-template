{
	"canvasType": "map",
	"coordinates": {
		"initialBounds": [-157,-32,75,67]
		,"autoInitialBounds": false
	}
	,"renderOrderBasedOn" : "=doc._ldata.start"
	,"backgrounds": [
		{
			"name": "OSM"
			,"type": "osm"
			,"options": {
				"url": "https://{a-c}.tile.openstreetmap.org/{z}/{x}/{y}.png"
			}
		}
                ,{
			"name": "Stamen Toner"
			,"type": "stamen"
			,"options": {
				"url": "https://stamen-tiles-{s}.a.ssl.fastly.net/toner/{z}/{x}/{y}{r}.png"
				,"layerName": "toner"
			}
		}
	]
	,"overlays": [
		{
			"id": "firstOverlay"
			,"name": {
				"nunaliit_type":"localized"
				,"en":"Demo"
			}
			,"type": "model"
			,"options": {
				"sourceModelId": "donut"
			}
			,"featurePopupDelayMs": 300
			,"useHoverSound": true
			,"clustering": {
				"distance": 20
				,"threshold": 1
				,"disableDynamicClustering": false
				,"clusterPointsOnly": false
				,"minimumLinePixelSize": 20
				,"minimumPolygonPixelSize": 20
			}

		}
	]
	,"addPointsOnly": false
	,"showSRSAttribution": true
	,"toggleClick": true
	,"styles":[
		{
			"condition": "onLayer('public')"
			,"label": "Public"
			,"normal": {
				"fill":"#ffff33"
			}
		}
		,{
			"condition": "hasProperty('doc._ldata')"
			,"normal": {
				"v2_style": true,
				"n2es6.n2mapModule.N2CustomPointStyle" :{
					"type": "treeRingB",
					"donutScaleFactor": "=doc._ldata.scaleFactor",
					"color": "=doc._ldata.style.fillColor",
					"stroke" : "#ffffff",
					"opacity": 0.8,
					"data": "=doc._ldata",
					"text" : "",
					"startupOffset" : 5
				}
			}
			,"selected": {
				"v2_style" : true,
				"n2es6.n2mapModule.N2CustomPointStyle" :{
					"type": "treeRingB",
					"donutScaleFactor": "=doc._ldata.scaleFactor",
					"color": "=doc._ldata.style.fillColor",
					"stroke" : "#ffffff",
					"opacity": 0.8,
					"data": "=doc._ldata",
					"text" : "",
					"startupOffset" : 5
				}

			}

		}
	]
}
